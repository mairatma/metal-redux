{
  "name": "metal-redux",
  "version": "2.0.1",
  "description": "A set of helpers for building projects using Metal.js and Redux",
  "license": "BSD",
  "repository": "metal/metal-redux",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "jsnext:main": "src/redux.js",
  "main": "lib/redux.js",
  "files": [
    "lib",
    "src",
    "test"
  ],
  "scripts": {
    "checkFormat": "npm run prettier -- --list-different",
    "compile": "babel -d lib/ src/",
    "format": "npm run prettier -- --write",
    "lint": "eslint '{src,test}/**/*.js'",
    "prepublish": "npm run compile",
    "prettier": "prettier-eslint '{src,test}/**/*.js'",
    "test": "karma start",
    "test:saucelabs": "karma start karma-saucelabs.conf.js"
  },
  "babel": {
    "presets": [
      "metal",
      "metal-jsx"
    ]
  },
  "keywords": [
    "metal"
  ],
  "dependencies": {
    "metal-incremental-dom": "^2.0.0",
    "metal-jsx": "^2.0.0",
    "metal-state": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-metal": "^4.0.0",
    "babel-preset-metal-jsx": "0.0.3",
    "chai": "^4.1.2",
    "eslint": "^4.15.0",
    "eslint-config-liferay": "^2.0.18",
    "eslint-plugin-react": "^7.5.1",
    "karma": "^1.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sauce-launcher": "~1.1.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "^2.0.6",
    "mocha": "^4.1.0",
    "prettier-eslint-cli": "^4.7.0",
    "sinon": "^4.1.4",
    "webpack": "^3.10.0"
  }
}
